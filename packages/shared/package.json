{
  "name": "@vue/shared",
  "version": "3.5.13",
  "description": "internal utils shared across @vue packages",
  "main": "index.js",
  "module": "dist/shared.esm-bundler.js",
  "types": "dist/shared.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "check": "depcruise src --config .dependency-cruiser.cjs",
    "check-deps": "depcruise src --config ../../.dependency-cruiser.js --output-type dot | dot -T svg > dependency-graph.svg",
    "check-test": "depcruise src1 --include-only \"^src1\" --output-type dot | dot -T svg > dependency-graph.svg",
    "check-madge": "madge src  --image graph.svg",
    "check-madge-npm": "madge src  --image graph.svg --include-npm"
  },
  "exports": {
    ".": {
      "types": "./dist/shared.d.ts",
      "node": {
        "production": "./dist/shared.cjs.prod.js",
        "development": "./dist/shared.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/shared.esm-bundler.js",
      "import": "./dist/shared.esm-bundler.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "buildOptions": {
    "formats": [
      "esm-bundler",
      "cjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/core.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "vue"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/core/issues"
  },
  "homepage": "https://github.com/vuejs/core/tree/main/packages/shared#readme"
}
